{
  "name": "@omlette-project-template/utilities",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/common.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "build:watch": "tsc --project ./tsconfig.json --watch --sourceMap",
    "test": "jest --config ./test/jest.config.npm.js",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:coverage:view": "yarn test:coverage --watchAll=false && yarn opener ./coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch --config ./test/jest.config.common.js"
  }
}
