{
  "name": "omlette-project-template",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "workspaces": [
    "scripts/**/*",
    "utilities/",
    "bin/"
  ],
  "main": "dist/bin/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch --sourceMap",
    "deploy": "",
    "test": "jest --config ./test/jest.config.all.js",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:coverage:view": "yarn test:coverage --watchAll=false && yarn opener ./coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch --config ./test/jest.config.common.js",
    "lint": "yarn tsc && yarn eslint:check",
    "eslint": "eslint --ignore-path .gitignore --ext ts -c ./bin/.eslintrc.js",
    "eslint:check": "yarn eslint bin/",
    "eslint:fix": "yarn eslint --fix bin/",
    "format": "prettier './bin/**'",
    "format:write": "prettier --write './bin/**'",
    "utilities": "yarn workspace @omlette-project-template/utilities run",
    "package-updates": "yarn workspace @omlette-project-template/package-updates run",
    "package-updates:install": "yarn workspace @omlette-project-template/package-updates add"
  },
  "author": "Christopher Ouellette",
  "license": "MIT",
  "bin": "dist/index.js",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.1.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^1.1.0",
    "msw": "^0.41.0",
    "nodemon": "^2.0.16",
    "opener": "^1.5.2",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
