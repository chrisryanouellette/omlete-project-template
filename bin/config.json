{
  "cliOpts": {
    "template": {
      "options": ["frontend", "backend", "web", "native"],
      "default": "web"
    },
    "project": {
      "default": "project-template"
    },
    "install": {
      "options": ["true", "false"],
      "default": "true"
    },
    "git": {
      "options": ["true", "false"],
      "default": "false"
    },
    "interactive": {
      "default": "false"
    }
  },
  "ignoreFolders": ["node_modules", "coverage", "dist", "lib", "build"]
}
